<Window x:Class="CDCasupanan.BingMaps.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:m="clr-namespace:Microsoft.Maps.MapControl.WPF;assembly=Microsoft.Maps.MapControl.WPF"
        xmlns:local="clr-namespace:CDCasupanan.BingMaps.Windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>

        <m:Map x:Name="myMap"
               Center="11.9997207, 121.7740173"
               ZoomLevel="5.4"
               MouseDoubleClick="myMap_MouseDoubleClick"
               CredentialsProvider="InsertYourBingMapsKey"
               
               />

    </Grid>
</Window>
